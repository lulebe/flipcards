{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source": "*(!(*.png|*.ico|*.svg|*.js|*.css|*map))",
        "destination": "/index.html"
      },
      {
        "source": "**/*(!(*.png|*.ico|*.svg|*.js|*.css|*map))",
        "destination": "/index.html"
      }
    ]
  }
}
